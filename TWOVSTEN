



#include <iostream>
using namespace std;

int main() {
     int t;
     cin >> t;
     while(t--){
          int x;
          cin >> x;
          int ans;
          if(x%10==0){
               ans=0;
          }else if(x%5==0){
               ans=1;
          }else{
               ans=-1;
          }
          
          cout << ans << endl;
      
          
     }
	// your code goes here
	return 0;
}
