


#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n],b[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    
	     for(int i=0;i<n;i++){
	        cin>>b[i];
	    }
	    int ans=INT_MIN;
	     for(int i=0;i<n;i++){
	        int w=a[i]*20-b[i]*10;
	        ans=max(ans,w);
	        
	    }
	    if(ans<0)
	    cout<<"0"<<endl;
	    else
	    cout<<ans<<endl;
	}
	return 0;
}
