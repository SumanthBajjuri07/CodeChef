


#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    string x,y;
	    cin>>x>>y;
	    int l=x.length();
	    int check=0;
	    for(int i=0;i<l;i++)
	    {
	        if(x[i]!=y[i])
	        {
	            if(x[i]!='?' && y[i]!='?')
	            {
	                check++;
	            }
	        }
	    }
	    if(check==0)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
