////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////


#include <iostream>
#include<cstring>
using namespace std;

int main() { long long t,n;
         cin>>t;
         
          while(t--)
          {
             string S;
              cin>>S;
           long long  l=S.length();
           
            
              long long z=0;
              long long o=0;
            
              for(int i=0;i<l;i++){
                  if(S[i]=='0'){
                      z++;
                  }
                  else
                  {
                      o++;
                  }
              } 
              
            
              
              if(o==1 || z==1)
              {
                  cout<<"Yes"<<endl;
              }
              else
              {
                  cout<<"No"<<endl;
              }
              
              
          }
	// your code goes here
	return 0;
}
